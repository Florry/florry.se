    {
        "id": "hallbarhetshjulet",
        "name": "Hållbarhetshjulet",
        "from": "2018",
        "to": "",
        "coverMedia": {
            "type": "IMAGE",
            "filename": "hallbarhetshjulet1.jpg"
        },
        "media": [
            {
                "type": "IMAGE",
